{
  "expo": {
    "name": "ecoBreath",
    "slug": "ecoBreath",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/LogoBrancaSplash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "ecobreath",
    "splash": {
      "image": "./assets/LogoAzul.png",
      "resizeMode": "contain",
      "backgroundColor": "#13D8B0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecoBreath.ecoBreathApp"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "comgooglemaps",
              "host": "reset-password"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoAzul.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jjoaosantos301270.ecoBreath"
    },
    "web": {
      "favicon": "./assets/LogoAzul.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#13D8B0"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2f7a9331-6d02-4f9a-9a48-ab7aab129d6c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2f7a9331-6d02-4f9a-9a48-ab7aab129d6c"
    }
  }
}
