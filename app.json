{
  "expo": {
    "name": "ecoBreath",
    "slug": "ecoBreath",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoBrancaSplash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "ecoBreath",
    "splash": {
      "image": "./assets/LogoAzul.png",
      "resizeMode": "contain",
      "backgroundColor": "#13D8B0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ecoBreath.ecoBreathApp"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "ecoBreath",
              "host": "reset-password"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoAzul.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jjoaosantos301270.ecoBreath"
    },
    "web": {
      "favicon": "./assets/LogoAzul.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#13D8B0"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2f7a9331-6d02-4f9a-9a48-ab7aab129d6c"
      }
    }
  }
}
