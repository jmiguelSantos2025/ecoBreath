{
  "expo": {
    "name": "ecoBreath",
    "slug": "ecoBreath",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoBrancaSplash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme":"ecoBreath",
    "splash": {
      "image": "./assets/LogoAzul.png",
      "resizeMode": "contain",
      "backgroundColor": "#13D8B0"
    },
    "ios": {
      "supportsTablet": true,
      "budleIdentifier": "com.ecoBreath.ecoBreathApp"
    },
    "android": {
      "intentFilters":{
        "action": "VIEW",
        "data":[
          {
            "scheme":"ecoBreath",
            "host":"reset-password"
          }
        ],
        "category":["BROWSABLE", "DEFAULT"]
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoAzul.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/LogoAzul.png"
    },
    "plugins": [
      "expo-router"
      
    ]
  }
}
